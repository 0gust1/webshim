<PUBLIC:COMPONENT lightWeight="true">


<PUBLIC:PROPERTY NAME="placeholder" PUT="placeholderSet" GET="placeholderGet" /></PUBLIC:COMPONENT>

<SCRIPT LANGUAGE="JScript">
var noop = function(){};


var get = function(name){
	return jQuery.attr(element, name);
};
var set = function(name, arg){
	return jQuery.attr(element, name, arg);
};



//placeholder
var placeholderSet = function(val){
	set('placeholder', val);
};
var placeholderGet = function(){
	return get('placeholder', element);
};
</SCRIPT>