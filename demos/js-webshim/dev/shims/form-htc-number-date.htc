<SCRIPT LANGUAGE="JScript">
var noop = function(){};


var get = function(name){
	return jQuery.attr(element, name);
};
var set = function(name, arg){
	return jQuery.attr(element, name, arg);
};


var valueAsNumberSet = function(val){
	set('valueAsNumber', val);
};
